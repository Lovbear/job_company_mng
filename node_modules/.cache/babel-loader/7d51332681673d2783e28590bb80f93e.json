) {\n\t\t\t          callback(new Error('请再次输入密码'));\n\t\t\t        } else if (value !== this.ruleForm.pass) {\n\t\t\t          callback(new Error('两次输入密码不一致!'));\n\t\t\t        } else {\n\t\t\t          callback();\n\t\t\t        }\n\t\t\t      };\n\t\t\t\t  \n\t\t\treturn {\n\t\t\t\truleForm:{\n\t\t\t\t\tuserName:'',\n\t\t\t\t\tpassword:''\n\t\t\t\t},\n\t\t\t\trules:{\n\t\t\t\t\t userName: [\n\t\t\t\t\t\t{ validator: checkName, trigger: 'blur' }\n\t\t\t\t\t  ],\n\t\t\t\t\t  password: [\n\t\t\t\t\t\t{ validator: checkPass, trigger: 'blur' }\n\t\t\t\t\t  ]\n\t\t\t\t}\n\t\t\t}\n\t\t},\n\t\tmethods:{\n\t\t\tregister(){\n\t\t\t\tthis.$router.push({\n\t\t\t\t\tpath:'/register'\n\t\t\t\t})\n\t\t\t},\n\t\t\tforgetPass(){\n\t\t\t\tthis.$router.push({\n\t\t\t\t\tpath:'/forgetPass'\n\t\t\t\t})\n\t\t\t},\n\t\t\tsubmitForm(formName) {\n\t\t\t\tthis.$refs[formName].validate((valid) => {\n\t\t\t\t  if (valid) {\n\t\t\t\t\talert('submit!');\n\t\t\t\t  } else {\n\t\t\t\t\tconsole.log('error submit!!');\n\t\t\t\t\treturn false;\n\t\t\t\t  }\n\t\t\t\t});\n\t\t\t}\n\t\t}\n\t}\n</script>\n\n<style scoped=\"scoped\" lang=\"less\">\n\t.login{\n\t\t.title{\n\t\t\tmargin-top:20%;\n\t\t\tfont-size: 48px;\n\t\t}\n\t\t.form{\n\t\t\tmargin:100px auto 0;\n\t\t\twidth:300px;\n\t\t}\n\t\t.submit{\n\t\t\twidth: 100%;\n\t\t}\n\t\t\n\t\t.info{\n\t\t\tjustify-content:flex-end;\n\t\t\t.blue{\n\t\t\t\tpadding: 0 10px;\n\t\t\t}\n\t\t}\n\t}\n</style>\n"],"sourceRoot":"src/views/login"}]}