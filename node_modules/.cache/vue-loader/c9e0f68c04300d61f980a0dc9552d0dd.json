e,password).then(res=>{\n\t\t\t\t\t\t\tsetToken(res.data.token);\n\t\t\t\t\t\t\tthis.$router.replace({\n\t\t\t\t\t\t\t\tpath:'/'\n\t\t\t\t\t\t\t})\n\t\t\t\t\t\t})\n\t\t\t\t  } else {\n\t\t\t\t\tconsole.log('error submit!!');\n\t\t\t\t\treturn false;\n\t\t\t\t  }\n\t\t\t\t});\n\t\t\t}\n\t\t}\n\t}\n</script>\n\n<style scoped=\"scoped\" lang=\"less\">\n\t.login{\n\t\t.title{\n\t\t\tmargin-top:20%;\n\t\t\tfont-size: 48px;\n\t\t}\n\t\t.form{\n\t\t\tmargin:100px auto 0;\n\t\t\twidth:300px;\n\t\t}\n\t\t.submit{\n\t\t\twidth: 100%;\n\t\t}\n\t\t\n\t\t.info{\n\t\t\tjustify-content:flex-end;\n\t\t\t.blue{\n\t\t\t\tpadding: 0 10px;\n\t\t\t}\n\t\t}\n\t}\n</style>\n"]}]}